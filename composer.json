{
  "name": "weslinkde/phpas2",
  "description": "PHPAS2 is a php-based implementation of the EDIINT AS2 standard",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladyslav K",
      "role": "Developer",
      "email": "vk.tiamo@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:weslinkde/phpas2.git"
    }
  ],
  "keywords": [
    "edi",
    "ediint",
    "as2",
    "server"
  ],
  "require": {
    "php": ">=5.5",
    "ext-zlib": "*",
    "ext-openssl": "*",
    "psr/log": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "phpseclib/phpseclib": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "AS2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AS2\\Tests\\": "tests/"
    }
  }
}
